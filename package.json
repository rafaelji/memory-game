{
  "name": "memory-game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "npx prettier . --write",
    "format:style": "npx stylelint \"**/*.css\" --fix",
